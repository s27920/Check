{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "S3Settings": {
    "Region": "eu-central-1",
    "BucketName": "beetcode-data-bucket"
  },
  "RabbitMq": {
    "HostName": "mq",
    "UserName": "guest",
    "Password": "guest",
    "Port": 5672
  },
  "ConnectionStrings": {
    "Redis": "redis:6379"
  },
  "Jwt": {
    "Issuer": "algoduck-dev",
    "Audience": "algoduck-spa-dev",
    "DurationInMinutes": 60,
    "ValidateIssuer": false,
    "ValidateAudience": false,
    "ValidateLifetime": true,
    "ClockSkewSeconds": 0,
    "RefreshDays": 14,
    "JwtCookieName": "jwt",
    "RefreshCookieName": "refresh_token",
    "CsrfCookieName": "csrf_token",
    "CsrfHeaderName": "X-CSRF-Token"
  },
  "Cors": {
    "DevOrigins": [
      "http://localhost:5173",
      "https://localhost:5173"
    ]
  },
  "SecurityHeaders": {
    "ImgOrigins": [
      "https://beetcode-data-bucket.s3.eu-central-1.amazonaws.com"
    ],
    "ConnectOrigins": [],
    "StyleOrigins": [
      "https://fonts.googleapis.com"
    ],
    "FontOrigins": [
      "https://fonts.gstatic.com"
    ],
    "ReportOnly": false
  },
  "Authentication": {
    "Google": {
      "CallbackPath": "/api/auth/oauth/google"
    },
    "GitHub": {
      "CallbackPath": "/api/auth/oauth/github"
    },
    "Facebook": {
      "CallbackPath": "/api/auth/oauth/facebook"
    }
  },
  "Email": {
    "Provider": "Postmark"
  }
}