{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RabbitMq": {
    "HostName": "mq",
    "UserName": "guest",
    "Password": "guest",
    "Port": 5672
  },
  "S3Settings": {
    "ContentBucketSettings": {
      "Region": "eu-central-1",
      "BucketName": "beetcode-data-bucket"
    },
    "DataBucketSettings": {
      "Region": "eu-central-1",
      "BucketName": "beetcode-content-bucket"
    }
  },
  "ConnectionStrings": {
    "Redis": "redis:6379"
  },
  "Jwt": {
    "Issuer": "algoduck",
    "Audience": "algoduck-client",
    "DurationInMinutes": 60,
    "ValidateIssuer": true,
    "ValidateAudience": true,
    "ValidateLifetime": true,
    "ClockSkewSeconds": 60,
    "RefreshDays": 30,
    "JwtCookieName": "jwt",
    "RefreshCookieName": "refresh_token",
    "CsrfCookieName": "csrf_token",
    "CsrfHeaderName": "X-CSRF-Token",
    "CookieDomain": "algoduck.com"
  },
  "Cors": {
    "ProdOrigins": [
      "https://algoduck.com"
    ]
  },
  "SecurityHeaders": {
    "ImgOrigins": [
      "https://beetcode-data-bucket.s3.eu-central-1.amazonaws.com"
    ],
    "ConnectOrigins": [
      "https://algoduck.com"
    ],
    "StyleOrigins": [
      "https://fonts.googleapis.com"
    ],
    "FontOrigins": [
      "https://fonts.gstatic.com"
    ],
    "ReportOnly": false
  },
  "Authentication": {
    "Google": {
      "CallbackPath": "/api/auth/oauth/google"
    },
    "GitHub": {
      "CallbackPath": "/api/auth/oauth/github"
    },
    "Facebook": {
      "CallbackPath": "/api/auth/oauth/facebook"
    }
  },
  "Email": {
    "Provider": "Postmark"
  }
}